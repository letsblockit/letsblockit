{
  "name": "webpack",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "css": "node-sass --output-style compressed --omit-source-map-url styles.scss ../../data/assets/css/styles.min.css",
    "fonts": "install node_modules/@fortawesome/fontawesome-free/webfonts/*.woff2 -t ../../data/assets/fonts/",
    "js": "install node_modules/htmx.org/dist/htmx.min.js node_modules/handlebars/dist/handlebars.min.js node_modules/bootstrap/dist/js/bootstrap.bundle.min.js -t ../../data/assets/js/",
    "assets": "npm run css && npm run fonts && npm run js"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^5.1.3",
    "handlebars": "^4.7.7",
    "htmx.org": "^1.6.1",
    "node-sass": "^7.0.1",
    "tar": ">=6.1.9",
    "trim-newlines": ">=3.0.1",
    "json-schema": ">=0.4.0"
  }
}
